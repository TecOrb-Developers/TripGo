	<!--feedback pop-up start here-->
	<div class="modal fade" id="myModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header model-head">
					<button type="button" class="close close-1" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
					<h4 class="modal-title" id="myModalLabel">Feedback</h4>
				</div>
				<div class="modal-body">
					<%= form_for :complaint,url: complaint_path,:html => {:class => "form-horizontal"} do |f| %>
					<div class="form-group">
						<label for="inputEmail3" class="col-sm-4 control-label ctrl-label ">Name</label>
						<div class="col-sm-8">
							<%= f.text_field :name,:class=>"form-control input_box1 input-box"%>
							<span class="complaint_name_err_field error"></span>
						</div>
					</div>

					<div class="form-group">
						<label for="inputPassword3" class="col-sm-4 control-label ctrl-label ">Contact No.</label>
						<div class="col-sm-8">
							<%= f.text_field :contact_no,:class=>"form-control input_box1 input-box"%>
							<span class="complaint_contact_no_err_field error"></span>
						</div>
					</div>

					<div class="form-group">
						<label for="inputEmail3" class="col-sm-4 control-label ctrl-label ">Email</label>
						<div class="col-sm-8">
							<%= f.text_field :email,:class=>"form-control input_box1 input-box"%>
							<span class="complaint_email_err_field error"></span>
						</div>
					</div>

					<div class="form-group">
						<label for="inputEmail3" class="col-sm-4 control-label ctrl-label ">Feedback Category</label>
						<div class="col-sm-8">

							<%=f.select :category, set_category,{},{ :class => 'my_style_class' }%>
							<span class="complaint_category_err_field error"></span>	
						</div>
					</div>

					<div class="form-group">
						<label for="inputEmail3" class="col-sm-4 control-label ctrl-label ">Subject</label>
						<div class="col-sm-8">
							<%= f.text_field :subject,:class=>"form-control input_box1 input-box" %>
							<span class="complaint_subject_err_field error"></span>
						</div>
					</div>

					<div class="form-group">
						<label for="inputEmail3" class="col-sm-4 control-label ctrl-label ">Message</label>
						<div class="col-sm-8">
							<%= f.text_area :message,:class=>"form-control input_box1 input-box" %>
							<span class="complaint_message_err_field error"></span>
						</div>
					</div>
					<div class="modal-footer model-ftr">
						<%= f.submit 'Submit',:class=>"btn btn-info blue-button btn-red complaint_submit"%>
					</div>

					<%end%>
				</div>
			</div>
		</div>
	</div>
	<!--feedback pop-up ends here-->